<div class="mockups">
  <%= partial 'partials/computer', locals: {large: (true unless project.onepage), image: (project.mockups.computer || project.main)} %>
  <div class="iphones">
    <% project.mockups.iphone.each_with_index do |image, i| %>
      <%= partial 'partials/iphone', locals: {id: i, image: image} %>
    <% end %>
  </div>
</div>
